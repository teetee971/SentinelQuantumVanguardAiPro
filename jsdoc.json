{
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": ["src", "functions"],
    "exclude": ["node_modules", "dist", "build"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/minami"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    },
    "systemName": "Sentinel Quantum Vanguard AI Pro",
    "footer": "<p>© 2025 Sentinel Systems – Tous droits réservés.</p>",
    "copyright": "2025 Sentinel Quantum Vanguard AI",
    "navType": "vertical",
    "theme": "dark",
    "linenums": true,
    "collapseSymbols": false
  },
  "tags": {
    "allowUnknownTags": true
  },
  "markdown": {
    "idInHeadings": true
  }
}
